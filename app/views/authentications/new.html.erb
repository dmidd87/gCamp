<div class="container">
    <div class="col-md-offset-4 col-md-4">
      <div class="well">

    <h2>Sign in to gCamp</h2>
    <%= form_tag signin_path do |f| %>

    <%= label_tag :email_address %>
    <%= email_field_tag :email_address, class:"form-control, col-sm-12" %>

    <%= label_tag :password %>
    <%= password_field_tag :password, class:"form-control, col-sm-12" %>

    <%= submit_tag "Sign In", :class => 'btn btn-primary'%>
  <% end %>

</div>
  </div>
      </div>
