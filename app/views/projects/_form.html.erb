<div class= "form-horizontal">

<%= render 'errorform/errorform' %>

<%= form_for @project do |f| %>
<div class="form-group">
        <%= f.label :name, "Name", class: "col-sm-2 control-label" %>
          <div class="col-sm-5">
            <%= f.text_field :name, class: "form-control" %>
          </div>
      </div>

      <div class="form-group">
        <div class="col-sm-offset-2 col<%  %>sm-5"> <br>
          <%= f.submit button_name:"Create Project", class: "btn btn-primary" %>
        </div>
      </div>
</div>
<% end %>
